{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*.tsx",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "REACT_APP_API_URL": "https://universeblog-api.onrender.com"
    }
  }
  